{
	"name": "@polyfiller/object-fit-demo",
	"private": true,
	"version": "0.0.39",
	"description": "A demo for the 'object-fit' and 'object-position' polyfill",
	"homepage": "https://object-fit.netlify.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/polyfiller-org/library.git"
	},
	"bugs": {
		"url": "https://github.com/polyfiller-org/library/issues"
	},
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"start": "sirv public",
		"validate": "svelte-check",
		"check": "tsc --noEmit",
		"lint": "pnpm run check && eslint \"src/**/*.ts\" --color",
		"update": "pnpx npm-check-updates --packageFile package.json -u -x np --dep dev,prod"
	},
	"devDependencies": {
		"@polyfiller/config": "workspace:*",
		"@polyfiller/object-fit": "workspace:*",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-image": "^2.1.1",
		"rollup-plugin-ts": "^2.0.4",
		"node-sass": "7.0.0",
		"postcss": "^8.4.5",
		"rollup": "^2.61.1",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.44.2",
		"svelte-check": "^2.2.10",
		"svelte-preprocess": "^4.10.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.3"
	},
	"dependencies": {
		"lolight": "^1.4.0",
		"sirv-cli": "^1.0.14"
	}
}
