{
	"name": "@polyfiller/object-fit-demo",
	"private": true,
	"version": "0.0.38",
	"description": "A demo for the 'object-fit' and 'object-position' polyfill",
	"homepage": "https://object-fit.netlify.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/polyfiller-org/library.git"
	},
	"bugs": {
		"url": "https://github.com/polyfiller-org/library/issues"
	},
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"start": "sirv public",
		"validate": "svelte-check",
		"check": "tsc --noEmit",
		"lint": "pnpm run check && eslint \"src/**/*.ts\" --color",
		"update": "pnpx npm-check-updates --packageFile package.json -u -x np --dep dev,prod"
	},
	"devDependencies": {
		"@polyfiller/config": "workspace:*",
		"@polyfiller/object-fit": "workspace:*",
		"@rollup/plugin-commonjs": "^15.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-image": "^2.0.5",
		"@wessberg/rollup-plugin-ts": "^1.3.5",
		"node-sass": "^4.14.1",
		"postcss": "^8.1.1",
		"rollup": "^2.28.2",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^6.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.29.0",
		"svelte-check": "^1.0.54",
		"svelte-preprocess": "^4.3.2",
		"tslib": "^2.0.1",
		"typescript": "^4.0.3"
	},
	"dependencies": {
		"lolight": "^1.4.0",
		"sirv-cli": "^1.0.6"
	}
}
